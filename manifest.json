{
  "manifest_version": 3,
  "name": "跨境ERP助手",
  "description": "跨境ERP助手，支持采集100+货源平台商品，批量发布至数十家跨境电商平台店铺，支持一键代发采购，自动关联并同步采购单至货代系统",
  "version": "2.6.16",
  "icons": {
    "16": "icon/16.png",
    "24": "icon/24.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "cookies",
    "declarativeNetRequestWithHostAccess",
    "webRequest",
    "management"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.szchengji-inc.com/*",
      "*://*.chengji-inc.com/*",
      "*://*.kuajing-plw.happyporsche.com/*",
      "*://*.91miaoshou.com/*"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/platform-base-utils.css",
        "env.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "platform-base-utils.js",
        "collect-platform-1688-unlisted.js",
        "collect-platform-2tong-unlisted.js",
        "collect-platform-aliexpress-unlisted.js",
        "collect-platform-bao66-unlisted.js",
        "collect-platform-coupang-unlisted.js",
        "collect-platform-gigab2b-unlisted.js",
        "collect-platform-haibei-unlisted.js",
        "collect-platform-juyiwang-unlisted.js",
